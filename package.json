{
  "name": "momat-server-express",
  "version": "0.0.1",
  "description": "momat backend with node+express",
  "main": "src/index.ts",
  "scripts": {
    "start": "babel-node -x '.ts' src/index.ts",
    "dev": "nodemon",
    "build:tsc": "tsc src/index.ts --noEmit false --outDir ./dist",
    "build:babel": "babel -o dist/index.js -x '.ts' -- src/index.ts"
  },
  "author": "Floyd Kim",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.7",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
